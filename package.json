{
  "name": "portage",
  "version": "0.0.1",
  "description": "Fast pub/sub for JS",
  "main": "dist/portage.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "fuzzytree": "0.0.3",
    "lodash": "^3.9.3"
  },
  "devDependencies": {
    "async": "^1.3.0",
    "babel": "^5.6.14",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "postal": "^1.0.6",
    "should": "^7.0.1"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --opts tests/mocha.opts tests",
